{
  // 使用 IntelliSense 了解相关属性。 
  // 悬停以查看现有属性的描述。
  // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) 启动",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/bin/bonc-backend-sat",
      // "args": [
      //   "--input",
      //   "/tmp/bonc-ui/20251227T083339-hfktbi/temp-out/bonc_rounds4.json",
      //   "-d",
      //   "plaintext=1"
      // ],
      "args": ["--differential", "/tmp/bonc-ui/20251228T080212-m52byk/temp-out/bonc_round6.json", "--input-bits=plaintext", "--solve", "--print-states='.*'"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "为 gdb 启用整齐打印",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "将反汇编风格设置为 Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },
  ]
}